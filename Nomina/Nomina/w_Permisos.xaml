<Window x:Class="Nomina.w_Permisos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nomina"
        mc:Ignorable="d"
        Title="w_Permisos" Height="300" Width="600" WindowStartupLocation="CenterScreen">
    <Canvas>
        <GroupBox Header=" Permisos solicitados:" Height="227" Width="572" Canvas.Left="10">
            <DataGrid x:Name="dgPermisos" Margin="0,0,-10,-4" Loaded="dgPermisos_Loaded"></DataGrid>
        </GroupBox>
        <Button x:Name="btnAprobar" Content="Aprobar" Click="btnAprobar_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Width="86" Canvas.Left="185" Canvas.Top="227"/>
        <Button x:Name="btnRechazar" Content="Rechazar" Click="btnRechazar_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Width="93" Canvas.Left="289" Canvas.Top="227"/>

    </Canvas>
</Window>
